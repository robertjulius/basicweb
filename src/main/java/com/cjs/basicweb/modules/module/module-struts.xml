<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="module" extends="app-default" namespace="/modules/module">

		<action name="initial" class="moduleMainAction" method="initial">
			<result name="success">/jsp/modules/module/module_maintenance_main.jsp</result>
		</action>

		<action name="prepareNew" class="moduleMainAction" method="prepareNew">
			<result name="success">/jsp/modules/module/module_maintenance_new.jsp</result>
		</action>

		<action name="confirmNew" class="moduleExecuteAction" method="confirmNew">
			<result name="success">/jsp/modules/module/module_maintenance_confirm.jsp</result>
		</action>

		<action name="search" class="moduleMainAction" method="search">
			<interceptor-ref name="defaultStackHibernate" />
			<result name="success">/jsp/modules/module/module_maintenance_search.jsp</result>
		</action>

		<action name="prepareDetail"
			class="com.cjs.basicweb.modules.module.action.ModuleMainAction"
			method="prepareDetail">
			<interceptor-ref name="defaultStackHibernate" />
			<result name="success">/jsp/modules/module/module_maintenance_detail.jsp</result>
		</action>
		
		<action name="prepareEdit"
			class="com.cjs.basicweb.modules.module.action.ModuleMainAction"
			method="prepareEdit">
			<result name="success">/jsp/modules/module/module_maintenance_edit.jsp</result>
		</action>

	</package>

</struts>
