<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="module" extends="app-default" namespace="/modules/module">

		<action name="initial"
			class="com.cjs.basicweb.modules.module.action.ModuleMainAction"
			method="initial">
			<result name="success">/jsp/modules/module/module_maintenance_main.jsp
			</result>
		</action>

		<action name="prepareNew"
			class="com.cjs.basicweb.modules.module.action.ModuleMainAction"
			method="prepareNew">
			<result name="success">/jsp/modules/module/module_maintenance_new.jsp
			</result>
		</action>

		<action name="confirmNew"
			class="com.cjs.basicweb.modules.module.action.ModuleMainAction"
			method="confirmNew">
			<result name="success">/jsp/modules/module/module_maintenance_confirm_new.jsp
			</result>
			<result name="error">/jsp/modules/module/module_maintenance_new.jsp
			</result>
		</action>
		
		<action name="executeNew"
			class="com.cjs.basicweb.modules.module.action.ModuleMainAction"
			method="executeNew">
			<result name="success">/jsp/modules/module/module_maintenance_main.jsp
			</result>
		</action>

		<action name="search"
			class="com.cjs.basicweb.modules.module.action.ModuleMainAction"
			method="search">
			<result name="success">/jsp/modules/module/module_maintenance_search.jsp
			</result>
		</action>

		<action name="prepareDetail"
			class="com.cjs.basicweb.modules.module.action.ModuleMainAction"
			method="prepareDetail">
			<result name="success">/jsp/modules/module/module_maintenance_detail.jsp
			</result>
		</action>

		<action name="prepareEdit"
			class="com.cjs.basicweb.modules.module.action.ModuleMainAction"
			method="prepareEdit">
			<result name="success">/jsp/modules/module/module_maintenance_edit.jsp
			</result>
		</action>

		<action name="confirmEdit"
			class="com.cjs.basicweb.modules.module.action.ModuleMainAction"
			method="confirmEdit">
			<result name="success">/jsp/modules/module/module_maintenance_confirm_edit.jsp
			</result>
			<result name="error">/jsp/modules/module/module_maintenance_edit.jsp
			</result>
		</action>
		
		<action name="executeEdit"
			class="com.cjs.basicweb.modules.module.action.ModuleMainAction"
			method="executeEdit">
			<result name="success">/jsp/modules/module/module_maintenance_main.jsp
			</result>
		</action>

	</package>

</struts>
