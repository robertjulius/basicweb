<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="module-main" extends="app-default" namespace="/modules/module">

		<action name="initial"
			class="com.cjs.basicweb.modules.module.action.ModuleMaintenanceMainAction"
			method="initial">
			<result name="success" type="redirectAction">main
			</result>
		</action>

		<action name="main"
			class="com.cjs.basicweb.modules.module.action.ModuleMaintenanceMainAction"
			method="redirectAction">
			<result name="success">/jsp/modules/module/module_maintenance_main.jsp
			</result>
		</action>

		<action name="search"
			class="com.cjs.basicweb.modules.module.action.ModuleMaintenanceMainAction"
			method="search">
			<result name="success" type="redirectAction">searchResult
			</result>
		</action>

		<action name="searchResult"
			class="com.cjs.basicweb.modules.module.action.ModuleMaintenanceMainAction"
			method="redirectAction">
			<result name="success">/jsp/modules/module/module_maintenance_search.jsp
			</result>
		</action>

		<action name="prepareDetail"
			class="com.cjs.basicweb.modules.module.action.ModuleMaintenanceMainAction"
			method="prepareDetail">
			<result name="success" type="redirectAction">detail
			</result>
		</action>

		<action name="detail"
			class="com.cjs.basicweb.modules.module.action.ModuleMaintenanceMainAction"
			method="redirectAction">
			<result name="success">/jsp/modules/module/module_maintenance_detail.jsp
			</result>
		</action>

	</package>

</struts>
