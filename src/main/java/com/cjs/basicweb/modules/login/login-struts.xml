<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="login" extends="app-default" namespace="/modules/login">

		<action name="executeLogin" class="loginExecuteAction">
			<interceptor-ref name="defaultStackHibernate" />
			<result name="input">/jsp/modules/login/login.jsp</result>
			<result name="success">/jsp/main/main.jsp</result>
		</action>

	</package>

</struts>
